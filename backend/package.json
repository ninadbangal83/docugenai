{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "DEBUG=* node dist/app.js",
    "dev": "nodemon --ext ts --exec node --loader ts-node/esm src/app.ts",
    "lint": "eslint . --ext .ts",
    "test": "NODE_OPTIONS=--loader=ts-node/esm jest",
    "test:watch": "NODE_OPTIONS=--loader=ts-node/esm jest --watch",
    "test:coverage": "NODE_OPTIONS=--loader=ts-node/esm jest --coverage",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "minio": "^8.0.5",
    "multer": "^2.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "eslint": "^9.30.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
