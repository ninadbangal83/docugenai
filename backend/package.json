{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "DEBUG=* node dist/index.js",
    "dev": "nodemon --ext ts --exec node --loader ts-node/esm src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "NODE_OPTIONS=--loader=ts-node/esm jest",
    "test:watch": "NODE_OPTIONS=--loader=ts-node/esm jest --watch",
    "test:coverage": "NODE_OPTIONS=--loader=ts-node/esm jest --coverage",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "file-type": "^21.0.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.9.1",
    "minio": "^8.0.5",
    "mongoose": "^8.16.1",
    "multer": "^2.0.1",
    "pdf-parse": "^1.1.1",
    "tesseract.js": "^6.0.1",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.10",
    "@types/pdf-parse": "^1.1.5",
    "@types/validator": "^13.15.2",
    "eslint": "^9.30.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
